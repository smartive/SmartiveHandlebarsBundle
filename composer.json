{
  "name": "smartive/handlebars-bundle",
  "description": "This bundle integrates rendering of Handlebars templates into Symfony2",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "smartive GmbH",
      "homepage": "https://www.smartive.ch"
    }
  ],
  "support": {
    "source": "https://github.com/smartive/SmartiveHandlebarsBundle"
  },
  "autoload": {
    "psr-4": { "Smartive\\HandlebarsBundle\\": "" }
  },
  "require": {
    "php": ">=5.3.9",
    "symfony/framework-bundle": "~2.6",
    "symfony/finder": "~2.6",
    "xamin/handlebars.php": "~0.10"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "satooshi/php-coveralls": "dev-master",
    "symfony/twig-bundle": "~2.6"
  },
  "suggest": {
    "symfony/twig-bundle": "If you want to use the Handlebars Twig extension to render Handlebars templates inside your Twig templates."
  },
  "config": {
    "bin-dir": "bin",
    "process-timeout": 1800
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
