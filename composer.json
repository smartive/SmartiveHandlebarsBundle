{
  "name": "smartive/handlebars-bundle",
  "description": "This bundle integrates rendering of Handlebars templates into Symfony2",
  "keywords": ["handlebars", "templating", "twig"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "smartive AG",
      "homepage": "https://smartive.ch"
    }
  ],
  "support": {
    "source": "https://github.com/smartive/SmartiveHandlebarsBundle"
  },
  "autoload": {
    "psr-4": { "Smartive\\HandlebarsBundle\\": "" }
  },
  "require": {
    "php": ">=5.4.0",
    "symfony/framework-bundle": "^2.5|^3.0",
    "symfony/finder": "^2.5|^3.0",
    "xamin/handlebars.php": "0.10.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0",
    "predis/predis": "^1.0",
    "satooshi/php-coveralls": "^1.0",
    "snc/redis-bundle": "^1.1|^2.0",
    "symfony/twig-bundle": "^2.5|^3.0",
    "twig/twig": "^1.30"
  },
  "suggest": {
    "snc/redis-bundle": "If you want to cache templates using Redis.",
    "symfony/twig-bundle": "If you want to use the Handlebars Twig extension to render Handlebars templates inside your Twig templates."
  },
  "config": {
    "bin-dir": "bin",
    "process-timeout": 1800
  },
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
